{
  "name": "@femessage/v-gantt",
  "version": "1.1.1",
  "description": "vue gantt component, just like omniplan!",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FEMessage/v-gantt.git"
  },
  "contributors": [
    "https://github.com/FEMessage"
  ],
  "keywords": [
    "vue",
    "element",
    "gantt",
    "omniplan"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/v-gantt.umd.min.js",
  "module": "dist/v-gantt.esm.min.js",
  "unpkg": "dist/v-gantt.umd.min.js",
  "browser": {
    "./sfc": "src/index.vue"
  },
  "scripts": {
    "dev": "vue-cli-service styleguidist",
    "build": "yarn build:umd & yarn build:esm & yarn doc",
    "build:umd": "vue-cli-service build --target lib src/main.ts",
    "build:esm": "rollup -c",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit --passWithNoTests",
    "lint": "vue-cli-service lint",
    "doc": "vue-cli-service styleguidist:build",
    "inspect": "vue-cli-service inspect > webpack.config.js",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release": "gren release --override"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dayjs": "^1.8.29",
    "lodash.clonedeep": "^4.5.0",
    "store2": "^2.11.2"
  },
  "devDependencies": {
    "@femessage/github-release-notes": "^0.19.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@types/jest": "^24.0.19",
    "@types/lodash.clonedeep": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/compiler-sfc": "^3.0.0-beta.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.2.11",
    "prettier": "^1.19.1",
    "rollup": "^2.21.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-vue": "^5",
    "standard-version": "^8.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "~3.9.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-styleguidist": "^4.26.0",
    "vue-template-compiler": "^2.6.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.@(md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.@(ts|js)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "gren": "@femessage/grenrc"
}
